{
  "name": "drupal-ambientimpact-core",
  "version": "2.0.0",
  "license": "GPL-2.0-or-later",
  "repository": "github:Ambient-Impact/drupal-ambientimpact-core",
  "scripts": {
    "build": "webpack --progress --config webpack.config.mjs",
    "build:vendor": "yarn run vendorize",
    "postinstall": "yarn run build:vendor"
  },
  "exports": {
    "./componentPaths": "./javascript/component_paths.js"
  },
  "yarn.build": {
    "input": [
      "package.json",
      "webpack.config.mjs",
      "**/components/**/*.scss"
    ],
    "output": [
      "**/components/**/*.css"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@consensus.enterprises/pnp-vendorize": "^1.0.0",
    "@symfony/webpack-encore": "^4.7.0",
    "autoprefixer": "^10.2.5",
    "css-loader": "^6.11.0",
    "file-loader": "^6.2.0",
    "glob": "^10.3.10",
    "mini-css-extract-plugin": "^2.9.1",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.0.1",
    "sass": "^1.43.4",
    "sass-loader": "^13.3.3",
    "sassy-lists": "^3.0.1",
    "webpack": "^5.94.0",
    "webpack-cli": "^4.10.0",
    "webpack-remove-empty-scripts": "^1.0.1"
  },
  "dependencies": {
    "ally.js": "^1.4.1",
    "fastdom": "^1.0.10",
    "include-media": "^1.4.10",
    "strictdom": "^1.0.1"
  },
  "vendorize": [
    "ally.js",
    "fastdom",
    "strictdom"
  ]
}
